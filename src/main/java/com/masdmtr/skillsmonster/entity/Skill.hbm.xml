<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.masdmtr.skillsmonster.entity.Skill" table="skill" schema="public" catalog="skillsmonster">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="code">
            <column name="code" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="keySkill">
            <column name="key_skill" sql-type="boolean" not-null="true"/>
        </property>
        <property name="collectionId">
            <column name="collection_id" sql-type="integer" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>