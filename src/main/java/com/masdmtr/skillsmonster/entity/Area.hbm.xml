<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.masdmtr.skillsmonster.entity.Area" table="area" schema="public" catalog="skillsmonster">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="country">
            <column name="country" sql-type="varchar(3)" length="3" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="code">
            <column name="code" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="zip">
            <column name="zip" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="extCode">
            <column name="ext_code" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>